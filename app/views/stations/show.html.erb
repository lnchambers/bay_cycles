<h2>Name: </h2>
<h3><%= @station.name %></h3>
<h2>Dock Count: </h2>
<h3><%= @station.dock_count %></h3>
<h2>City: </h2>
<h3><%= @station.city %></h3>
<h2>Installation Date: </h2>
<h3><%= @station.installation_date %></h3>
<h3><%= render partial: "analytics" if current_user && has_trips? %></h3>
<h3><%= link_to "Edit", edit_admin_station_path(@station) if current_admin? %></h3>
<h3><%= link_to "Delete", admin_station_path(@station), method: "delete" if current_admin? %></h3>
