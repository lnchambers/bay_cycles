<br>
<section class="button">
  <h3><%= link_to "Create New Station", new_admin_station_path if current_admin? %></h3>
</section>
<section class="trips-dashboard-analytics">
    <% @stations.each do |station| %>
      <section class="trips-dashboard-value">
        <h3><%= link_to station.name, station_path(station) %></h3>
        Dock Count: <%= station.dock_count %><br>
        City: <%= station.city %><br>
        Installation Date: <%= station.installation_date %>
        <%= link_to "Edit", edit_admin_station_path(station) if current_admin? %>
        <%= link_to "Delete", admin_station_path(station), method: "delete" if current_admin? %>
      </section>
    <% end %>
</section>
