<section class="trips-dashboard-analytics">
  <% @trips.each do |trip| %>
    <section class="trips-dashboard-value">
      <h2>Trip ID: <%= trip.id %></h2> <br>
      <h3><%= trip.duration %></h3> <br>
      <h3><%= trip.start_date %><h3> <br>
      <h3><%= trip.start_station.name %><h3> <br>
      <h3><%= trip.end_date %><h3> <br>
      <h3><%= trip.end_station.name %><h3> <br>
      <h3><%= trip.bike_id %><h3> <br>
      <h3><%= trip.subscription_type %><h3> <br>
      <h3><%= trip.zip_code %><h3> <br>
      <h3><%= link_to "More Info?", trip_path(trip) %><h3>
      <h3><%= link_to "Edit Trip", edit_admin_trip_path(trip) if current_admin? %><h3>
      <h3><%= link_to "Delete Trip", admin_trip_path(trip), method: "delete" if current_admin? %><h3>
    </section>
  <% end %>
</section>
<%= paginate @trips %> <br>
