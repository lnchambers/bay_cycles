<% @conditions.each do |condition| %>
  Date: <%= condition.date %>
  Max Temperature: <%= condition.max_temperature %>
  Mean Temperature: <%= condition.mean_temperature %>
  Min Temperature: <%= condition.min_temperature %>
  Humidity: <%= condition.mean_humidity %>
  Mean Visibility: <%= condition.mean_visibility %> miles
  Mean Wind Speed: <%= condition.mean_wind_speed %>mph
  Precipitation: <%= condition.precipitation %> inches
<% end %>